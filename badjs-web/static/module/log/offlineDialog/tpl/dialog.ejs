<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" id="offlineConfigModal">
    <div class="modal-backdrop fade"></div>
    <div class="modal-dialog">
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true" data-event-click="close">×</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">设置离线日志自动拉取</h4>
            </div>
            <div class="modal-body">
                <div class="form-group" style="text-align: center;">
                    <input style="display: inline-block;width: 300px;" type="text" class="form-control" id="addUin" placeholder="添加监听的UIN">
                    <button type="submit" class="btn btn-default" data-event-click="addUin">添加</button>
                </div>
                <div class="uin-list">
                    <table class="table table-striped">
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-event-click="close">Close</button>
            </div>

        </div>
    </div>
</div>
